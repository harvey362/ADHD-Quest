{
  "name": "adhd-quest",
  "version": "1.0.0",
  "private": true,
  "description": "Comprehensive cross-platform productivity suite for ADHD and executive dysfunction",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@supabase/supabase-js": "^2.39.3",
    "chart.js": "^4.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-toastify": "^10.0.4",
    "react-scripts": "5.0.1",
    "framer-motion": "^11.0.3",
    "ical-generator": "^7.0.0",
    "papaparse": "^5.4.1",
    "jszip": "^3.10.1",
    "jwt-decode": "^4.0.0",
    "date-fns": "^3.3.1",
    "localforage": "^1.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@playwright/test": "^1.40.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "babel-jest": "^29.7.0",
    "msw": "^2.0.11",
    "fast-check": "^3.15.0",
    "axe-core": "^4.8.3",
    "@axe-core/playwright": "^4.8.3",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.13.0",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test --grep @a11y",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lighthouse": "lhci autorun",
    "security:audit": "npm audit --production",
    "security:check": "npm run security:audit && npm run lint",
    "ci": "npm run lint && npm run type-check && npm run test && npm run build",
    "ci:full": "npm run ci && npm run test:e2e && npm run lighthouse",
    "eject": "react-scripts eject",
    "mobile": "cd mobile && expo start",
    "mobile:android": "cd mobile && expo run:android",
    "mobile:ios": "cd mobile && expo run:ios",
    "precommit": "npm run format:check && npm run lint && npm run type-check",
    "prepush": "npm run test"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  }
}
